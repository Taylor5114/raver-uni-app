
.top{
	background: -webkit-linear-gradient(top, #c84e89, #F15F79);
	background: linear-gradient(to bottom, #c84e89, #F15F79);
	width: 750rpx;
	height: 400rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
}
._top{
	width: 750rpx;
	height: 60rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: end;
	-webkit-justify-content: flex-end;
	        justify-content: flex-end;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
._top image{
	width: 40rpx;
	height: 40rpx;
}
._top navigator{
	margin-right: 30rpx;
}
._top1{
	width: 750rpx;
	-webkit-box-flex: 1;
	-webkit-flex-grow: 1;
	        flex-grow: 1;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-pack: space-evenly;
	-webkit-justify-content: space-evenly;
	        justify-content: space-evenly;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}

