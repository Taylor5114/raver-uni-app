
.content{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
}
.top{
	width: 100%;
	height: 100rpx;
	background-color: #37C2BB;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	color: #8DD8D5;
}
.main{
	-webkit-box-flex: 1;
	-webkit-flex-grow: 1;
	        flex-grow: 1;
}
.navlist{
	width: 90%;
	height: 100%;
	margin-left: 5%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.navlist text{
	margin-right: 20rpx;
}
.change{
	font-size: 40rpx;
	color: #FFFFFF;
}

