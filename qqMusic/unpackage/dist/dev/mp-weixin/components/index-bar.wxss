
.tt{
	font-family: "等线";
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	background-color: #fafafa;
}
.nav{
	height: 70rpx;
	list-style: none;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	/* justify-content: space-around; */
	/* overflow-x: auto; */
}
.nav-list{
	height: 70rpx;
	list-style: none;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	/* overflow-x: auto; */
}
.nav-list ._li{
	height: 40rpx;
	color: #757575;
	margin: 0 20rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.nav-list ._li:hover{
	color: #272727;
	font-size: 40rpx;
	border-bottom: 6rpx solid #38E6AF;
}
.nav-right{
	width: 50rpx;
	height: 50rpx;
	background-color: #007AFF;
}
.nav-right image{
	width: 100%;
	height: 100%;
}
.tt-search{
	width: 750rpx;
	height: 80rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.search{
	width: 680rpx;
	height: 60rpx;
	background-color: #e8e8e8;
	-webkit-border-radius: 30rpx;
	        border-radius: 30rpx;
}
#search-btn{
	width: 60rpx;
	height: 60rpx;
	background: url(data:image/png;base64,) no-repeat center;
	background-size: 60%;
}
.body{
	width: 750rpx;
	height: 1232rpx;
	background-color: #fafafa;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}

