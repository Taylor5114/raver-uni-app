
.content{
	width: 200rpx;
	height: 200rpx;
	position: relative;
}
.tt{
	width: 100%;
	height: 100%;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
}
._tt{
	width: 120rpx;
	height: 120rpx;
	position: absolute; left: 50%; top: 50%;   
	-webkit-transform: translate(-50%, -50%);   
	        transform: translate(-50%, -50%);
	z-index: 1;
}
.ttbg{
	background: -webkit-gradient(linear, left top, left bottom, from(#8BDEDA), color-stop(#43ADD0), color-stop(#998EE0), color-stop(#E17DC2), to(#EF9393));
	background: -webkit-linear-gradient(top, #8BDEDA, #43ADD0, #998EE0, #E17DC2, #EF9393);
	background: linear-gradient(to bottom, #8BDEDA, #43ADD0, #998EE0, #E17DC2, #EF9393);
}

