
.content{
	width: 160rpx;
	height: 160rpx;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
	position: relative;
}
.tt{
	width: 100%;
	height: 100%;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
}
._tt{
	width: 120rpx;
	height: 120rpx;
	position: absolute; left: 50%; top: 50%;   
	-webkit-transform: translate(-50%, -50%);   
	        transform: translate(-50%, -50%);
	z-index: 1;
}
.ttbg{
	background: -webkit-gradient(linear, left top, left bottom, from(#4284DB), to(#29EAC4));
	background: -webkit-linear-gradient(top, #4284DB, #29EAC4);
	background: linear-gradient(to bottom, #4284DB, #29EAC4);
}

